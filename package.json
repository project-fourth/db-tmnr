{
  "name": "db-tmnr",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev-all": "concurrently \"cd db-tmnr-server && npm run dev\" \"cd db-tmnr-client && npm run dev\"",
    "install-all": "concurrently \"npm install\" \"cd db-tmnr-server && npm install\" \"cd db-tmnr-client && npm install\"",
    "build-server": "cd db-tmnr-server && npm run build",
    "preview-server": "cd db-tmnr-server && npm start",
    "build-client": "cd db-tmnr-client && npm run build",
    "preview-client": "cd db-tmnr-client && npm run preview",
    "build-all": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "preview-all": "concurrently \"npm run preview-server\" \"npm run preview-client\""
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
